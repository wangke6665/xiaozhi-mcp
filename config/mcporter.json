{
  "mcpServers": {
    "xiaozhi": {
      "command": "node",
      "args": ["/root/.openclaw/workspace/xiaozhi-mcp-bridge.js"],
      "env": {
        "XIAOZHI_MCP_URL": "${XIAOZHI_MCP_URL}"
      }
    },
    "comfyui": {
      "command": "node",
      "args": ["/root/.openclaw/workspace/comfyui-mcp-server.js"],
